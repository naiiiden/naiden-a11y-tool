{
  "manifest_version": 3,
  "name": "naiden's a11y tool",
  "description": "naiden's accessibility audit tool",
  "version": "1.1",
  "permissions": [
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "devtools_page": "devtools.html"
}